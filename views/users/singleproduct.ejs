                      <script>
                      function shareProductOnWhatsApp() {
                          // Get the uploaded photo
                          const photo = document.getElementById('uploadedPhoto').src;
                          
                          // Get the current page URL
                          const pageURL = window.location.href;
                          
                          // Define the message to share
                          const message = `${pageURL}`;
                          
                          // URL to open WhatsApp with the predefined message and photo
                          const whatsappURL = `https://wa.me/?text=${encodeURIComponent(message)}%0A&image=${encodeURIComponent(photo)}`;
                          
                          // Open the URL in a new window
                          window.open(whatsappURL, '_blank');
                      }
                      
                      // Handle photo upload
                      document.getElementById('photoInput').addEventListener('change', function() {
                          const file = this.files[0];
                          
                          if (file) {
                              const reader = new FileReader();
                              
                              reader.addEventListener('load', function() {
                                  // Set the uploaded photo as the source of the img element
                                  document.getElementById('uploadedPhoto').src = this.result;
                              });
                              
                              reader.readAsDataURL(file);
                          }
                      });
                      </script>
